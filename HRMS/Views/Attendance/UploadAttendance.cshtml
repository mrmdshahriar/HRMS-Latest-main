
@{
    ViewBag.Title = "UploadAttendance";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link rel="stylesheet" href="~/Content/Assets/vendors/mdi/css/materialdesignicons.min.css">
<link rel="stylesheet" href="~/Content/Assets/vendors/base/vendor.bundle.base.css">

<link href="~/Content/Assets/vendors/datatables.net-bs4/dataTables.bootstrap4.css" rel="stylesheet" />

<!-- End plugin css for this page -->
<!-- inject:css -->
<link href="~/Content/Assets/css/vertical-layout-light/style.css" rel="stylesheet" />

<link href="~/Content/Assets/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

<link href="~/Content/Assets/css/Region.css" rel="stylesheet" />
<br />
<br />

<div class="container">
    <!-- Button trigger modal -->
    <!-- Modal -->
    <!-- Country List -->
    <div class="row">
        <div class="col-12">
            <h3 class="all-md pt-3">Upload Attendance</h3>
            <hr />
            @using (Html.BeginForm("UploadAttendance", "Attendance", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                if (TempData["CheckData"] != null)
                {
                    if (Convert.ToString(TempData["CheckData"]) == "success")
                    {
                        <script>
                                Swal.fire({
                                    icon: 'Success',
                                    text: "Data Has been saved Successfully"
                                });
                        </script>
                    }
                    else
                    {
                        <script>
                            Swal.fire({
                                icon: 'warning',
                                text: "SomeThing went Wrong!!!!!"
                            });
                        </script>
                    }
                }
                <div class="row">
                    <div class="col-md-4">
                        @*<input type="file" id="fileupload" name="file" />*@
                        @Html.TextBox("file", "", new { type = "file" })
                    </div>
                    <div class="col-md-4">
                        <input type="submit" class="btn btn-success" value="upload" id="btnsumbit" />
                    </div>
                </div>
            }
        </div>

    </div>
    <!-- Country List ends -->
    <!-- container-scroller -->
</div>


